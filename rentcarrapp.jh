
entity Merchant {
   companyName String required
   brandName String
   inn String 
   owner String
   phone String
   address String
}

// Set pagination options
paginate Merchant with pagination

// Use Data Transfer Objects (DTO)
dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

